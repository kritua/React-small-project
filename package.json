        "hook-scroll": "1.1.1",
        "isomorphic-fetch": "^2.2.1",
        "json-server": "^0.15.0",
        "lodash.throttle": "^4.1.1",
        "name-all-modules-plugin": "1.0.1",
        "node-fetch": "^2.6.0",
        "pretty-error": "2.1.1",
        "prop-types": "^15.7.2",
        "react": "^15.x.x",
        "react-dom": "^15.x.x",
        "react-helmet": "5.2.1",
        "react-redux": "^5.x.x",
        "react-router-async": "0.6.x",
        "redux": "^3.x.x",
        "redux-actions-helpers": "^1.0.4",
        "redux-form": "^7.x.x",

{
    "name": "vezsnab-web",
    "private": true,
    "version": "1.0.0",
    "license": "UNLICENSED",
    "main": "server/index.js",
    "scripts": {
        "build:dev": "concurrently -c \"blue,green\" -p [{name}] -n \"client,server\" \"yarn build:dev:client\" \"yarn build:dev:server\"",
        "build:dev:client": "NODE_ENV=development DEBUG=app:*,booker-ui:* DEBUG_COLORS=true PORT=4001 webpack --config=webpack",
        "build:dev:server": "NODE_ENV=development DEBUG=app:*,booker-ui:* DEBUG_COLORS=true PORT=4001 webpack --env.server --config=webpack",
        "start:dev": "DISABLE_CLUSTERIZE=true DEBUG=app:*,booker-ui:* DEBUG_COLORS=true NODE_ENV=development PORT=4001 node --trace-warnings `pwd`/server/build/index.js",
        "start": "DEBUG=app:*,booker-ui:* DEBUG_COLORS=true NODE_ENV=production PORT=4001 node `pwd`/server/build/index.js",
        "development:client": "NODE_ENV=development DEBUG=app:*,booker-ui:* DEBUG_COLORS=true PORT=4001 webpack-dev-server --config=webpack --hot --inline --trace-deprecation",
        "development:server": "yarn build:dev:server --watch",
        "development": "concurrently -k -c \"blue,green\" -p [{name}] -n \"server,client\" \"yarn development:server\" \"yarn development:client\"",
        "eslint": "eslint .",
        "eslint:fix": "eslint . --fix",
        "stylelint": "stylelint app/**/*.pcss"
    },
    "dependencies": {
        "body-parser": "^1.18.3",
        "classnames": "2.2.5",
        "compression": "^1.7.3",
        "cookie": "0.3.1",
        "cookie-parser": "1.4.3",
        "express": "^4.16.4",
        "history": "4.6.3",
        "hook-fetcher": "0.2.x",
        "hook-redux": "0.5.x",
        "hook-scroll": "1.0.2",
        "imagemin": "^6.0.0",
        "isomorphic-fetch": "^2.2.1",
        "lodash.intersection": "^4.4.0",
        "lodash.throttle": "^4.1.1",
        "name-all-modules-plugin": "1.0.1",
        "node-fetch": "^2.1.2",
        "pretty-error": "^2.1.1",
        "prop-types": "^15.6.2",
        "react": "^16.6.0",
        "react-dom": "^16.6.0",
        "react-helmet": "5.1.3",
        "react-redux": "5.0.5",
        "react-router-async": "^0.7.9",
        "react-transition-group": "^2.4.0",
        "redux": "3.7.2",
        "redux-actions-helpers": "^1.0.4",
        "redux-form": "^7.0.3",
        "redux-saga": "0.15.5",
        "redux-thunk": "2.2.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.1.2",
        "@babel/core": "^7.1.2",
        "@babel/plugin-proposal-class-properties": "^7.1.0",
        "@babel/plugin-proposal-decorators": "^7.1.2",
        "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
        "@babel/polyfill": "^7.0.0",
        "@babel/preset-env": "^7.1.0",
        "@babel/preset-react": "^7.0.0",
        "acorn": "^6.0.2",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.4",
        "clean-webpack-plugin": "^0.1.19",
        "compression-webpack-plugin": "^2.0.0",
        "concurrently": "3.5.0",
        "css-loader": "^1.0.0",
        "daemon-command-webpack-plugin": "1.4.5",
        "doiuse": "^4.2.0",
        "eslint": "^5.8.0",
        "eslint-loader": "^2.1.1",
        "eslint-plugin-react": "^7.11.1",
        "file-loader": "^2.0.0",
        "img-loader": "^3.0.1",
        "mini-css-extract-plugin": "^0.4.4",
        "postcss-loader": "^3.0.0",
        "postcss-nested": "2.0.4",
        "redux-logger": "3.0.6",
        "style-loader": "^0.23.1",
        "stylelint": "^9.6.0",
        "url-loader": "^1.1.2",
        "webpack": "4.23.1",
        "webpack-cli": "^3.1.2",

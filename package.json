{
  "name": "vezsnab-web",
  "private": true,
  "version": "1.0.0",
  "license": "UNLICENSED",
  "main": "server/index.js",
  "scripts": {
    "build:dev": "concurrently -c \"blue,green\" -p [{name}] -n \"client,server\" \"yarn build:dev:client\" \"yarn build:dev:server\"",
    "build:dev:client": "NODE_ENV=development DEBUG=app:*,booker-ui:* DEBUG_COLORS=true PORT=4001 webpack --config=webpack",
    "build:dev:server": "NODE_ENV=development DEBUG=app:*,booker-ui:* DEBUG_COLORS=true PORT=4001 webpack --env.server --config=webpack",
    "start:dev": "DISABLE_CLUSTERIZE=true DEBUG=app:*,booker-ui:* DEBUG_COLORS=true NODE_ENV=development PORT=4001 node --trace-warnings `pwd`/server/build/index.js",
    "start": "DEBUG=app:*,booker-ui:* DEBUG_COLORS=true NODE_ENV=production PORT=4001 node `pwd`/server/build/index.js",
    "development:client": "NODE_ENV=development DEBUG=app:*,booker-ui:* DEBUG_COLORS=true PORT=4001 webpack-dev-server --config=webpack --hot --inline",
    "development:server": "yarn build:dev:server -- --watch",
    "development": "concurrently -k -c \"blue,green\" -p [{name}] -n \"server,client\" \"yarn development:server\" \"yarn development:client\"",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint app/**/*.pcss"
  },
  "dependencies": {
    "body-parser": "1.17.2",
    "classnames": "2.2.5",
    "compression": "1.7.0",
    "cookie": "0.3.1",
    "cookie-parser": "1.4.3",
    "express": "4.15.3",
    "history": "4.6.3",
    "hook-fetcher": "0.2.x",
    "hook-redux": "0.5.x",
    "hook-scroll": "1.0.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash.intersection": "^4.4.0",
    "name-all-modules-plugin": "1.0.1",
    "node-fetch": "^2.1.2",
    "pretty-error": "2.1.1",
    "prop-types": "^15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-helmet": "5.1.3",
    "react-redux": "5.0.5",
    "react-router-async": "0.6.x",
    "redux": "3.7.2",
    "redux-actions-helpers": "^1.0.4",
    "redux-form": "^7.0.3",
    "redux-saga": "0.15.5",
